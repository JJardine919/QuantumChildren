MQL5 Side (StanozololDMTBridge.mqh):

    #include <StanozololDMTBridge.mqh>

    StanozololDMTBridge bridge;

    void OnInit()
    {
        bridge.SetSignalFile("stanozolol_dmt_signal.json");
        bridge.SetStaleTimeout(120);
    }

    void OnTick()
    {
        if(!bridge.ReadSignal()) return;

        STANO_Signal sig = bridge.GetSignal();

        if(sig.recommendation == "BOOST" && sig.adjusted_confidence > 0.70)
        {
            // Execute high-confidence boosted trade
            double confidence = sig.adjusted_confidence;
            int gates = sig.gates_passed;
            string regime = sig.regime;

            Print("BRIDGE BOOST: conf=", confidence,
                  " gates=", gates, "/13",
                  " regime=", regime);
        }
        else if(sig.recommendation == "SUPPRESS")
        {
            Print("BRIDGE SUPPRESS: ", sig.gates_passed, "/13 gates");
            // Skip this signal
        }
    }


